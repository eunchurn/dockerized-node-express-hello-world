{
  "name": "dockerized-node-express-hello-world",
  "version": "0.0.6",
  "description": "Dockerized nodejs express hello world example",
  "keywords": [],
  "license": "MIT",
  "author": "Eunchurn Park",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "dotenv -e .env.example -- ts-node-dev src/index.ts",
    "prepare": "husky install || true",
    "test": "dotenv -e .env.example -- jest",
    "typecheck": "tsc --noEmit",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "prettier": "@eunchurn/prettier-config",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@eunchurn/eslint-config": "^0.1.13",
    "@eunchurn/prettier-config": "^0.0.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/module-alias": "^2.0.2",
    "@types/node": "^20.6.2",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "husky": "8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "15.2.10",
    "pretty-quick": "4.0.0",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.2.2"
  }
}
